  "ct3aMetadata": {
    "initVersion": "7.40.0"
  },
  "packageManager": "pnpm@10.12.4",
  "lavamoat": {
    "allowScripts": {
      "playwright": true,
      "esbuild": true,
      "@esbuild/darwin-arm64": true,
      "@esbuild/darwin-x64": true,
      "@esbuild/linux-x64": true,
      "sharp": false,
      "bigint-buffer": false,
      "bufferutil": false,
      "utf-8-validate": false,
      "unrs-resolver": false,
      "msw": false,
      "@lavamoat/preinstall-always-fail": false
    }
  }
