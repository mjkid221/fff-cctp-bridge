{
  "name": "fff-cctp-bridge",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "safe-install": "pnpm install && pnpm exec allow-scripts",
    "allow-scripts": "pnpm exec allow-scripts",
    "allow-scripts:generate": "pnpm exec allow-scripts auto",
    "allow-scripts:analyze": "can-i-ignore-scripts",
    "analyze": "ANALYZE=true pnpm build",
    "build": "next build && pnpm build-storybook",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "pretest": "playwright install --with-deps chromium",
    "knip": "knip",
    "audit": "pnpm audit --audit-level=high",
    "audit:fix": "pnpm audit --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
